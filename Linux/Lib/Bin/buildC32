#!/bin/bash
cd ../Obj

Lib=../libOfront.a
if [ -f $Lib ]; then rm $Lib; fi
rm ../Sym/*.sym

## Ofront+
../Bin/compileC32 $Lib SYSTEM
../Bin/compileC32 $Lib Args
../Bin/compileC32 $Lib Console
../Bin/compileC32 $Lib Unix
../Bin/compileC32 $Lib Kernel.Unix
../Bin/compileC32 $Lib Files.Unix
../Bin/compileC32 $Lib Reals
../Bin/compileC32 $Lib Modules.Unix
../Bin/compileC32 $Lib CmdlnTexts

## OOC2
../Bin/compileC32 $Lib ooc2RandomNumbers

#../Bin/clear
