#!/bin/bash
cd ../Obj

Lib=../libOfront.a
if [ -f $Lib ]; then rm $Lib; fi
rm ../Sym/*.sym

## Ofront+
../Bin/compile $Lib SYSTEM
../Bin/compile $Lib Args
../Bin/compile $Lib Console
../Bin/compile $Lib Unix
../Bin/compile $Lib Kernel.Unix
../Bin/compile $Lib Files.Unix
../Bin/compile $Lib Reals
../Bin/compile $Lib Modules.Unix
../Bin/compile $Lib CmdlnTexts

## OOC2
../Bin/compile $Lib ooc2RandomNumbers

#../Bin/clear

