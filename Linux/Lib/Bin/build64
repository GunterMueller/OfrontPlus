#!/bin/bash
cd ../Obj64

Lib=../libOfront64.a
if [ -f $Lib ]; then rm $Lib; fi
rm ../Sym64/*.sym

## Ofront+
../Bin/compile64 $Lib SYSTEM
../Bin/compile64 $Lib Args
../Bin/compile64 $Lib Console
../Bin/compile64 $Lib Unix
../Bin/compile64 $Lib Kernel.Unix
../Bin/compile64 $Lib Files.Unix
../Bin/compile64 $Lib Reals
../Bin/compile64 $Lib Modules.Unix
../Bin/compile64 $Lib CmdlnTexts

## OOC2
../Bin/compile64 $Lib ooc2RandomNumbers

#../Bin/clear
