#!/bin/sh
cd ../Obj
export PATH="$PATH:.."
export OBERON=.:../Sym:../Lib/Sym:../../Mod

ofront+ -sC -44 OfrontErrors.Mod OfrontOPM.cmdln.Mod OfrontOPS.Mod OfrontOPT.Mod OfrontOPB.Mod OfrontOPP.Mod OfrontOPC.Mod OfrontOPV.Mod BrowserCmd.Mod -m ImportGraph.Mod -m OCatCmd.Mod -m OfrontCmd.Mod -m
if [ $? != 0 ]; then exit 1; fi

find . -type f -name "*.sym" -exec mv -f {} ../Sym \;
