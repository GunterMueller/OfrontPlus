#!/bin/sh
cd ../Obj
PATH="$PATH:.."
export OBERON=.:../Sym:../Lib/Sym:../../../Mod

ofront+ -sC -88 OfrontErrors.Mod OfrontOPM.cmdln.Mod OfrontOPS.Mod OfrontOPT.Mod OfrontOPB.Mod OfrontOPP.Mod OfrontOPC.Mod OfrontOPV.Mod BrowserCmd.Mod -m ImportGraph.Mod -m OCatCmd.Mod -m OfrontCmd.Mod -m OdcToText.Mod -3m OEF.Ob2 -2m
if [ $? != 0 ]; then exit 1; fi

find . -type f -name "*.sym" -exec mv -f {} ../Sym \;
